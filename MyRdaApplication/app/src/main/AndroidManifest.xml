<?xml version="1.0" encoding="utf-8" ?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="eu.epfc.myrdaapplication">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/tasbih2"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyRdaApplication">
        <activity
            android:name=".MainFirstActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity" android:label="@string/dhikr"
            android:exported="true" android:parentActivityName=".MainFirstActivity">

        </activity>
        <activity
            android:name=".PrayOfActivity" android:label="@string/prayOf"
            android:exported="true" android:parentActivityName=".MainFirstActivity">

        </activity>
    </application>

</manifest>